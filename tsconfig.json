{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ES6",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["node","vitest/globals"],
    "strictNullChecks":true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
  },
  "include": ["**/*.ts", "**/*.d.ts", "vite.config.ts", "vite.config.ts"],
}
