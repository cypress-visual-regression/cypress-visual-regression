{
  "compilerOptions": {
    "moduleResolution": "Bundler",
    "module": "ES6",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["node", "vitest/globals", "cypress"],
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist"
  },
  "include": ["**/*.ts"]
}
