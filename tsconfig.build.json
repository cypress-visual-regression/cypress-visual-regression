{
  "compilerOptions": {
    "declaration": true,
    "moduleResolution": "Bundler",
    "module": "ES6",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["node", "cypress", ],
    "strictNullChecks":true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/**/*.spec.ts"]
}
